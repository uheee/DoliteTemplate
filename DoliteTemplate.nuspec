<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    
    <id>Dolite.Templates.Api.Solution</id>
    <version>${RELEASE_VERSION}</version>
    <title>Dolite Web API Solution Template</title>
    <authors>Snowind</authors>
    <description>An out-of-box Web API solution template for .NET 7</description>
    <releaseNotes>
      Initial released.
    </releaseNotes>

    <projectUrl>https://github.com/uheee/DoliteTemplate</projectUrl>
    <repository type="git" url="https://github.com/uheee/DoliteTemplate.git" branch="master" />

    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>dolite solution template csharp dotnet web-api asp.net</tags>
    <icon>icon.png</icon>
    <readme>README.md</readme>

    <packageTypes>
      <packageType name="Template" />
