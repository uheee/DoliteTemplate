  <files>
    <file src=".template.config/icon.png" />
    <file src="README.md" />
    <file src="./**" target="content" exclude="**/bin/**;**/obj/**;**/.vs/**;**/.idea/**;**/.git/**;**/.github/**;**/LICENSE;**/README.md;" />
